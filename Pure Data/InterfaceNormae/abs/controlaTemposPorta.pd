tempo) \; local out2 = ofTable('set' \, max) \; local out3 = ofTable('set'
\, min \, max) \; out:outletList(0 \, out0) \; out:outletList(1 \,
out1) \; out:outletList(2 \, out2) \; out:outletList(3 \, out3) \;
end \;;
